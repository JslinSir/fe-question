.splash-view{
  position: fixed;
  width: 100vw;
  height: 100vh;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 99999;
  background-color: #fff;
}

.splash-box{
 width: 100vw;
 height: 800rpx;
 display: flex;
 flex-direction: column;
 align-items: center;
 justify-content: center;
 background-color: #fff;

}
.splash-icon{
  width: 690rpx;
  height: 800rpx;
}
.splash-title{
  position: fixed;
  bottom:120rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size: 32rpx;
  color: #323232;
  font-weight: bold;
  margin-top: 60rpx;
}

